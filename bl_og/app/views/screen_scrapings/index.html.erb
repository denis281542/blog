<p id="notice"><%= notice %></p>

<h1>Screen Scrapings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @screen_scrapings.each do |screen_scraping| %>
      <tr>
        <td><%= link_to 'Show', screen_scraping %></td>
        <td><%= link_to 'Edit', edit_screen_scraping_path(screen_scraping) %></td>
        <td><%= link_to 'Destroy', screen_scraping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Screen Scraping', new_screen_scraping_path %>
